#include<stdio.h>
#include<stdlib.h>
int main()
{
int a[10][10],b[10][10],mul[10][10],r,c,i,j,k;
clrscr();
printf("enter the number of rows =   ");
scanf("%d",&r);
printf("enter the number of columns = ");
scanf("%d",&c);
printf("enter the first matrix elements \n");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
scanf("%d";&a[i][j]);
}
}
printf("enter the second matrix elements \m");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
scanf("%d", b[i][j]);
}
}
printf("multiply of the matrix");
for(i=0;i<r;i++)
{
for(j=0;j<c;j++)
{
mul[i][j]=0;
for(k=0;k<c;k++)
{
printf("%d\t",mul[i][j]);
}
printf("\n");
}
return0;
}
